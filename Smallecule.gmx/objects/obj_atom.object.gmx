            <string>///Rotate
if (((_isMolecule == true) &amp;&amp; (_isLead == true)) &amp;&amp; (_active == true))
{
    var coll = collision_point(x, y, obj_rotator, 0, false);
    
    if (coll &gt; 0)
    {
        angle = coll._rotationAngle;
        
        //Rotate each
        var i;
        for (i = 0; i &lt; ds_list_size(_bondList); i++)
        {
            var atom = ds_list_find_value(_bondList, i);
            var rotAngle, dist;
            rotAngle = -angle;
            
            _rotating = true;
             
            if (instance_exists(atom))
            {
            switch(_direction)
            {
                case "":
                    rotAngle += point_direction(x, y, atom.x, atom.y);
                    dist = point_distance(x, y, atom.x, atom.y);
                    TweenFire(atom, x__, EaseOutQuad, 0, false, 0, obj_control._turnTimer / 2, atom.x, x + lengthdir_x(dist, rotAngle));
                    TweenFire(atom, y__, EaseOutQuad, 0, false, 0, obj_control._turnTimer / 2, atom.y, y + lengthdir_y(dist, rotAngle));
                    break;
                    
                case "right":
                    rotAngle += point_direction(x + obj_control._gridSize, y, atom.x + obj_control._gridSize, atom.y);
                    dist = point_distance(x + obj_control._gridSize, y, atom.x + obj_control._gridSize, atom.y);
                    TweenFire(atom, x__, EaseOutQuad, 0, false, 0, obj_control._turnTimer / 2, atom.x, x + obj_control._gridSize + lengthdir_x(dist, rotAngle));
                    TweenFire(atom, y__, EaseOutQuad, 0, false, 0, obj_control._turnTimer / 2, atom.y, y + lengthdir_y(dist, rotAngle));
                    break;
                    
                case "left":
                    rotAngle += point_direction(x - obj_control._gridSize, y, atom.x - obj_control._gridSize, atom.y);
                    dist = point_distance(x - obj_control._gridSize, y, atom.x - obj_control._gridSize, atom.y);
                    TweenFire(atom, x__, EaseOutQuad, 0, false, 0, obj_control._turnTimer / 2, atom.x, x - obj_control._gridSize + lengthdir_x(dist, rotAngle));
                    TweenFire(atom, y__, EaseOutQuad, 0, false, 0, obj_control._turnTimer / 2, atom.y, y + lengthdir_y(dist, rotAngle));
                    break;
                    
                case "up":
                    rotAngle += point_direction(x, y - obj_control._gridSize, atom.x, atom.y - obj_control._gridSize);
                    dist = point_distance(x, y - obj_control._gridSize, atom.x, atom.y - obj_control._gridSize);
                    TweenFire(atom, x__, EaseOutQuad, 0, false, 0, obj_control._turnTimer / 2, atom.x, x + lengthdir_x(dist, rotAngle));
                    TweenFire(atom, y__, EaseOutQuad, 0, false, 0, obj_control._turnTimer / 2, atom.y, y - obj_control._gridSize + lengthdir_y(dist, rotAngle));
                    break;
                    
                case "down":
                    rotAngle += point_direction(x, y + obj_control._gridSize, atom.x, atom.y + obj_control._gridSize);
                    dist = point_distance(x, y + obj_control._gridSize, atom.x, atom.y + obj_control._gridSize);
                    TweenFire(atom, x__, EaseOutQuad, 0, false, 0, obj_control._turnTimer / 2, atom.x, x + lengthdir_x(dist, rotAngle));
                    TweenFire(atom, y__, EaseOutQuad, 0, false, 0, obj_control._turnTimer / 2, atom.y, y + obj_control._gridSize + lengthdir_y(dist, rotAngle));
                    break;
            }
            }
        }
    }
}
